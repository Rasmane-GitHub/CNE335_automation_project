import pandas
from sqlalchemy import create_engine
from sqlalchemy import text
import pymysql


hostname="127.0.0.1"
uname="root"
pwd=""
dbname="MyFoodData"


 install pym#ysql
engine = create_engine("mysql+pymysql://{user}:{pw}@{host}/{db}"
            .format(host=hostname, db=dbname, user=uname, pw=pwd))

#Import MyFoodData website address
tables = pandas.read_excel (https://docs.google.com/spreadsheets/d/1snqE6leDkZlL61qQ4g-vUmiFjizJyN1OCVAhwWWKSm4/edit#gid=2024304766, sheet_name='Data 1',
                            header = 2, index_col='Date')

#
tables = pandas.read_excel ('https://www.eia.gov/dnav/pet/hist_xls/EMD_EPD2DXL0_PTE_R5XCA_DPGw.xls', sheet_name='Data 1',
                                    header = 2, index_col='Date')
tables.rename(columns = {'Weekly West Coast (PADD 5) Except California No 2 Diesel Ultra Low Sulfur (0-15 ppm) Retail Prices  (Dollars per Gallon)'
                         :'Diesel_Price'}, inplace=True)

connection=engine.connect()
tables.to_sql('fuel', con = engine, if_exists = 'append')

connection.execute(text('CREATE TABLE fuel_temp_2 Like fuel'))
connection.execute(text('INSERT INTO fuel_temp_2 SELECT DISTINCT (Date), Diesel_Price FROM fuel '))
connection.execute(text('DROP TABLE fuel'))
connection.execute(text('ALTER TABLE fuel_temp_2 RENAME TO fuel'))

connection.close() #Close extension to database